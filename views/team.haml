:javascript
  $(document).ready(function () {
    function getPreviousUrl(s) {
      var sp = s.split('/');
      var previous_url = sp[0] + '/' + sp[1] + '/' + sp[2];
      return previous_url;
    }


    var team_number = $('span#hidden-number').html();
    $('table tr td:contains("' + team_number + '")').css('font-weight', 'bold');

    $('#sidebar-previous').bind('click', function () {
      alert(document.location.pathname);
      document.location = getPreviousUrl(document.location.pathname);
    });
  });

%h2= "Team #: #{@team_number}"
%hr
%table.pretty
  %tr
    %td Team Rank:
    %td= @team_stats[0]
  %tr
    %td Matches Played:
    %td= @team_stats[2]
  %tr
    %td Seeding Score:
    %td= @team_stats[3]
  %tr
    %td Coopertition Bonus:
    %td= @team_stats[4]
  %tr
    %td Hanging Points:
    %td= @team_stats[5]

%h2 Match History
%hr
- @team_match_results.reverse.each do |match_info|
  %br
  %table.pretty
    %tr
      %th.match Match #
      %th Red Alliance
      %th Blue Alliance
      %th.score Score
    
    %tr
      %td= match_info[1]
      %td.teams= "#{match_info[2]}, #{match_info[3]}, #{match_info[4]}"
      %td.teams= "#{match_info[5]}, #{match_info[6]}, #{match_info[7]}"
      %td= "#{match_info[8]} - #{match_info[9]}"
%span#hidden-number.hidden= "#{@team_number}"
